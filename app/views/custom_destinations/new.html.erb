<h1>Create A New Destination!</h1>

<%= form_for(@custom_destination) do |f| %>
<%= f.label :name%>
<%= f.text_field :name %><br>
<%= f.label :address %>
<%= f.text_field :address %><br>
<%= f.label :year_created %>
<%= f.number_field :year_created %><br>
<%= f.label :destination_type %>
<%= f.text_field :destination_type %><br>
<%= f.submit %>
<% end %>
