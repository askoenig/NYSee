<h1>My Visited Places:</h1>
<div class="visits_display">
<% @visits.each do |v| %>
<li><%= link_to v.destination.name, destination_path(v.destination_id) %></li>
<% end %>
<% @custom_visits.each do |v| %>
<li><%= link_to v.custom_destination.name, custom_destination_path(v.custom_destination_path) %></li>
<% end %><br>
</div>
<%= button_to "Back To My Profile", user_path(session[:user_id]), :method => "get" %>
