<%= form_for(@user) do |f| %>
<%= f.label :first_name%>
<%= f.text_field :first_name %><br>
<%= f.label :last_name%>
<%= f.text_field :last_name %><br>
<%= f.label :age %>
<%= f.number_field :age %><br>
<%= f.label :username %>
<%= f.text_field :username %><br>
<%= f.label :password %>
<%= f.number_field :password %><br>
<%= f.label :email  %>
<%= f.number_field :email %><br>
<%= f.submit %>
<% end %>
