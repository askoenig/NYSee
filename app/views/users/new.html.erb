<%= form_for(@user) do |f| %>
<%= f.label :first_name%>
<%= f.text_field :first_name %><br>
<%= f.label :last_name%>
<%= f.text_field :last_name %><br>
<%= f.label :age %>
<%= f.number_field :age %><br>
<%= f.label :user_name %>
<%= f.text_field :user_name %><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.label :email  %>
<%= f.text_field :email %><br>
<%= f.submit %>
<% end %>
